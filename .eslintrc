{
  "root": true,
  "extends": "",
  "plugins": [
    "eslint-plugin-import"
  ],
  "rules": {
    "no-bitwise": "off",
    "no-lonely-if": "off",
    "class-methods-use-this": "off",
    "arrow-body-style": "off",
    "no-loop-func": "off",
    "no-param-reassign": [
      "off"
    ],
    "import/prefer-default-export": "off"
  }
}